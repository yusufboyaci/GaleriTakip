
@{
    ViewData["Title"] = "Arabalar";
}

<h1>Araba Listesi</h1>
<table id="ArabaListesi"
       data-toggle="ArabaListesi"
       data-click-to-select="true"
       data-show-colums="true"
       data-checkbox-header="false"
       data-multiple-select-row="true"
       data-toolbar="#toolbar"
       data-pagination="true"
       data-search="true">
    <thead>
        <tr>
            <th id="chk" data-field="state" data-checkbox-header="true"></th>
            <th data-field="ad"></th>
            <th data-field="ucret"></th>
            <th data-field="stok"></th>
            <th data-field="birim"></th>
            <th data-field="galeriId"></th>
        </tr>
    </thead>
    <tbody><tr></tr></tbody>
</table>
